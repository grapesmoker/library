<table class="table table-striped" id="documents" width="100%">
    <thead>
    <tr>
        <th>Document Info</th>
        <th>
          <span class="text-left">Actions</span>
          <span class="text-right">
            <select class="form-control" id="bulk-actions">
              <option value="rename">Rename</option>
              <option value="delete">Delete</option>
            </select>
          </span>
        </th>
    </tr>
    </thead>
    <tbody id="documents-body">

    </tbody>
    <tfoot>
        <tr>
            <td colspan="2">
                <ul class="pagination">
                    <li class="page-item"><a class="page-link" id="library-previous" href="#">Previous</a></li>
                    {{#each pages}}
                        <li class="page-item">
                            <a class="page-link" id="library-page-{{this}}" href="#">{{this}}</a>
                        </li>
                    {{/each}}
                    <li class="page-item"><a class="page-link" id="library-next" href="#">Next</a></li>
                </ul>
            </td>
        </tr>
    </tfoot>
</table>

